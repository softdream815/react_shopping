{
  "name": "cezerin",
  "version": "0.30.0",
  "description": "React and Node.js based e-commerce platform. https://cezerin.com",
  "keywords": [
    "cezerin",
    "ecommerce",
    "shopping cart",
    "shop",
    "cms"
  ],
  "homepage": "https://cezerin.com",
  "bugs": "https://github.com/cezerin/cezerin/issues",
  "license": "MIT",
  "author": {
    "name": "Restmount <jsonmount@gmail.com>",
    "url": "https://github.com/restmount"
  },
  "contributors": [
    {
      "name": "Restmount <jsonmount@gmail.com>",
      "url": "https://github.com/restmount"
    }
  ],
  "main": "src/api/server/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/cezerin/cezerin"
  },
  "scripts": {
    "compile:dev": "babel src/admin -d dist/admin --watch & babel src/store -d dist/store --watch & babel theme/src -d theme/dist --watch",
    "compile": "babel src/admin -d dist/admin --quiet && babel src/store -d dist/store --quiet",
    "webpack:admin:dev": "webpack -d --config webpack.config.admin.js --watch --progress --colors",
    "webpack:store:dev": "webpack -d --config webpack.config.store.js --watch --progress --colors",
    "webpack:admin:prod": "NODE_ENV=production webpack -p --config webpack.config.admin.js",
    "webpack:store:prod": "NODE_ENV=production webpack -p --config webpack.config.store.js",
    "theme:install": "./scripts/theme-install.sh",
    "theme:export": "./scripts/theme-export.sh",
    "theme:copy": "cd ./theme && npm run build --quiet && npm i && cd ../",
    "theme:build:dev": "npm run theme:copy && npm run webpack:store:dev",
    "theme:build:prod": "npm run theme:copy --silent && npm run webpack:store:prod",
    "build:dev": "npm run compile:dev & npm run webpack:store:dev & npm run webpack:admin:dev",
    "build": "NODE_ENV=production npm run compile && npm run theme:build:prod && npm run webpack:admin:prod",
    "start-api": "node src/api/server/index.js",
    "start-store": "node dist/store/server/index.js",
    "start": "NODE_ENV=production npm run start-api & npm run start-store"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.2",
    "cezerin-client": "^0.27.1",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.28.9",
    "express": "^4.16.2",
    "express-jwt": "^5.3.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "formidable": "^1.1.1",
    "fs-extra": "^5.0.0",
    "handlebars": "^4.0.11",
    "helmet": "^3.10.0",
    "html-webpack-plugin": "^2.30.1",
    "json-loader": "^0.5.7",
    "jsonwebtoken": "^8.1.1",
    "lodash": "^4.17.4",
    "lru-cache": "^4.1.1",
    "material-ui": "^0.20.0",
    "moment": "^2.20.1",
    "mongodb": "^3.0.1",
    "node-sass": "^4.7.2",
    "nodemailer": "^4.4.2",
    "nodemailer-smtp-transport": "^2.7.4",
    "query-string": "^5.0.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-dropzone": "^4.2.7",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scroll": "^1.7.6",
    "react-sortable-hoc": "^0.6.8",
    "react-tagsinput": "^3.19.0",
    "redux": "^3.7.2",
    "redux-form": "^7.2.1",
    "redux-form-material-ui": "^4.3.3",
    "redux-thunk": "^2.2.0",
    "response-time": "^2.3.2",
    "sass-loader": "^6.0.6",
    "script-ext-html-webpack-plugin": "^1.8.8",
    "sitemap": "^1.13.0",
    "slug": "^0.9.1",
    "theme": "file:theme",
    "ua-parser-js": "^0.7.17",
    "webpack": "^3.10.0",
    "winston": "^2.4.0"
  },
  "engines": {
    "node": "^8.0.0",
    "npm": "^5.0.0"
  }
}
